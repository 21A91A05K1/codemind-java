import java.util.*;
import java.lang.Math;
class Power
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a,b,c,d;
        a=sc.nextInt();
        b=sc.nextInt();
        c=sc.nextInt();
        d=(int)Math.pow(a,b);
        System.out.println(d%c);
    }
}
